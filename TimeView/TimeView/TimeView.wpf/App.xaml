<Application x:Class="TimeView.wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TimeView.wpf"
             xmlns:converter="clr-namespace:TimeView.wpf.Converters"
             StartupUri="View/Login.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:ViewModelLocator x:Key="mainViewModelLocator" />
            <converter:CompanyConverter x:Key="companyConverter" />
            <converter:CategoryEntryHourConverter x:Key="categoryEntryHourConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>